{
  "name": "resolve-did",
  "version": "1.1.0",
  "description": "Resolve DIDs from the command line and in code",
  "homepage": "https://github.com/catena-labs/resolve-did#readme",
  "bugs": "https://github.com/catena-labs/resolve-did/issues",
  "license": "MIT",
  "author": {
    "name": "Catena Labs",
    "url": "https://catenalabs.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/catena-labs/resolve-did.git"
  },
  "bin": {
    "resolve-did": "dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "tsdown src/index.ts src/cli.ts --dts",
    "check": "bun run build && bun format:check && bun lint && bun typecheck && bun run test && bun publint",
    "clean": "git clean -fdX .turbo dist",
    "cli": "bun src/cli.ts",
    "fix": "bun format && bun lint:fix",
    "format": "oxfmt .",
    "format:check": "oxfmt --check .",
    "lint": "oxlint .",
    "lint:fix": "bun lint -- --fix --fix-suggestions",
    "nuke": "bun run clean && git clean -xdf node_modules",
    "outdated": "bunx npm-check-updates --interactive --format group",
    "prepublishOnly": "bun run build",
    "publint": "publint",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@agentcommercekit/did": "^0.10.1"
  },
  "devDependencies": {
    "@types/bun": "^1.3.9",
    "@types/node": "^25.2.3",
    "oxfmt": "^0.32.0",
    "oxlint": "^1.47.0",
    "oxlint-tsgolint": "^0.12.2",
    "publint": "^0.3.17",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3"
  }
}
